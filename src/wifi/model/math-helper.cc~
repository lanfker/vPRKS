            <td class="blob-line-code"><div class="code-body highlight"><pre><div class='line' id='LC1'><span class="cp">#include &quot;math-helper.h&quot;</span></div><div class='line' id='LC2'><span class="cp">#include &lt;cmath&gt;</span></div><div class='line' id='LC3'><span class="cp">#include &quot;ns3/log.h&quot;</span></div><div class='line' id='LC4'><span class="c1">//#include &lt;gsl/gsl_cdf.h&gt;</span></div><div class='line' id='LC5'><span class="cp">#include &lt;cstddef&gt;</span></div><div class='line' id='LC6'><br/></div><div class='line' id='LC7'><span class="n">NS_LOG_COMPONENT_DEFINE</span> <span class="p">(</span><span class="s">&quot;MathHelper&quot;</span><span class="p">);</span></div><div class='line' id='LC8'><span class="k">namespace</span> <span class="n">ns3</span></div><div class='line' id='LC9'><span class="p">{</span></div><div class='line' id='LC10'><br/></div><div class='line' id='LC11'><span class="n">NS_OBJECT_ENSURE_REGISTERED</span> <span class="p">(</span><span class="n">MathHelper</span><span class="p">);</span></div><div class='line' id='LC12'><br/></div><div class='line' id='LC13'><span class="n">TypeId</span> <span class="n">MathHelper</span><span class="o">::</span><span class="n">GetTypeId</span><span class="p">(</span><span class="kt">void</span><span class="p">)</span></div><div class='line' id='LC14'><span class="p">{</span></div><div class='line' id='LC15'>&nbsp;&nbsp;<span class="k">static</span> <span class="n">TypeId</span> <span class="n">tid</span> <span class="o">=</span> <span class="n">TypeId</span> <span class="p">(</span><span class="s">&quot;ns3::MathHelper&quot;</span><span class="p">)</span></div><div class='line' id='LC16'>&nbsp;&nbsp;&nbsp;&nbsp;<span class="p">.</span><span class="n">SetParent</span><span class="o">&lt;</span><span class="n">Object</span><span class="o">&gt;</span> <span class="p">()</span></div><div class='line' id='LC17'>&nbsp;&nbsp;&nbsp;&nbsp;<span class="p">.</span><span class="n">AddConstructor</span><span class="o">&lt;</span><span class="n">MathHelper</span><span class="o">&gt;</span> <span class="p">()</span></div><div class='line' id='LC18'>&nbsp;&nbsp;&nbsp;&nbsp;<span class="p">;</span></div><div class='line' id='LC19'>&nbsp;&nbsp;<span class="k">return</span> <span class="n">tid</span><span class="p">;</span></div><div class='line' id='LC20'><span class="p">}</span></div><div class='line' id='LC21'><br/></div><div class='line' id='LC22'><span class="n">MathHelper</span><span class="o">::</span><span class="n">MathHelper</span> <span class="p">()</span></div><div class='line' id='LC23'><span class="p">{</span></div><div class='line' id='LC24'><br/></div><div class='line' id='LC25'>&nbsp;&nbsp;<span class="c1">//m_2_SqrtPi = 1.12837916709551257389615890312;</span></div><div class='line' id='LC26'>&nbsp;&nbsp;</div><div class='line' id='LC27'>&nbsp;&nbsp;<span class="c1">//m_Sqrt1_2 = 0.70710678118654752440084436210;</span></div><div class='line' id='LC28'>&nbsp;&nbsp;<span class="c1">//m_1_Sqrt2Pi = (m_2_SqrtPi * m_Sqrt1_2 / 2.0);</span></div><div class='line' id='LC29'>&nbsp;&nbsp;<span class="c1">//m_Gsl_Dbl_Epsilon = 2.2204460492503131e-16;</span></div><div class='line' id='LC30'>&nbsp;&nbsp;<span class="c1">//m_Sqrt2 = 1.41421356237309504880168872421; </span></div><div class='line' id='LC31'>&nbsp;&nbsp;<span class="c1">//m_Gauss_Epsilon = m_Gsl_Dbl_Epsilon / 2;</span></div><div class='line' id='LC32'>&nbsp;&nbsp;<span class="c1">//m_Gauss_XUpper  = 8.572;</span></div><div class='line' id='LC33'>&nbsp;&nbsp;<span class="c1">//m_Gauss_XLower = -37.519;</span></div><div class='line' id='LC34'>&nbsp;&nbsp;<span class="c1">//m_Gauss_Scale = 16.0;</span></div><div class='line' id='LC35'>&nbsp;&nbsp;<span class="n">m_P_t</span> <span class="o">=</span> <span class="o">-</span><span class="mi">15</span><span class="p">;</span></div><div class='line' id='LC36'>&nbsp;&nbsp;<span class="n">m_d0</span> <span class="o">=</span> <span class="mi">1</span><span class="p">;</span></div><div class='line' id='LC37'>&nbsp;&nbsp;<span class="n">m_PL_d0</span> <span class="o">=</span> <span class="mi">55</span><span class="p">;</span></div><div class='line' id='LC38'>&nbsp;&nbsp;<span class="n">m_sigma</span> <span class="o">=</span> <span class="mi">3</span><span class="p">;</span></div><div class='line' id='LC39'>&nbsp;&nbsp;<span class="n">m_B_N</span> <span class="o">=</span> <span class="mi">2000</span><span class="p">;</span></div><div class='line' id='LC40'>&nbsp;&nbsp;<span class="n">m_R</span> <span class="o">=</span> <span class="mi">250</span> <span class="o">*</span> <span class="mi">8</span><span class="p">;</span> </div><div class='line' id='LC41'>&nbsp;&nbsp;<span class="c1">//m_Sqrt32 = 4.0 * m_Sqrt2;</span></div><div class='line' id='LC42'>&nbsp;&nbsp;<span class="n">m_f</span> <span class="o">=</span> <span class="mi">132</span><span class="p">;</span></div><div class='line' id='LC43'>&nbsp;&nbsp;<span class="n">m_l</span> <span class="o">=</span> <span class="mi">5</span><span class="p">;</span></div><div class='line' id='LC44'>&nbsp;&nbsp;<span class="n">NS_LOG_FUNCTION</span> <span class="p">(</span><span class="k">this</span><span class="p">);</span></div><div class='line' id='LC45'><span class="p">}</span></div><div class='line' id='LC46'><br/></div><div class='line' id='LC47'><br/></div><div class='line' id='LC48'><br/></div><div class='line' id='LC49'><span class="kt">double</span> <span class="n">MathHelper</span><span class="o">::</span><span class="n">PdrToGamma</span> <span class="p">(</span><span class="kt">double</span> <span class="n">pdr</span><span class="p">,</span> <span class="kt">uint32_t</span> <span class="n">length</span><span class="p">)</span></div><div class='line' id='LC50'><span class="p">{</span></div><div class='line' id='LC51'>&nbsp;&nbsp;<span class="c1">// if pdr =1, the value of gamma will be infinity.</span></div><div class='line' id='LC52'>&nbsp;&nbsp;<span clas